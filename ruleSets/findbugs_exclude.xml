<FindBugsFilter>
    <Match>
        <Class name="~.*\.EJS.*|.*\._.*"/>
    </Match>
    <!-- Do not complain about getters returning mutable objects -->
    <Match>
        <Or>
            <Bug pattern="EI_EXPOSE_REP"/>
            <Bug pattern="EI_EXPOSE_REP2"/>
            <Bug pattern="DM_DEFAULT_ENCODING" />
        </Or>
    </Match>
</FindBugsFilter>
